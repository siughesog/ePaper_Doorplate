{
  "version": 2,
  "builds": [
    {
      "src": "frontend/epaper-doorplate-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/epaper-doorplate-frontend/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/epaper-doorplate-frontend/index.html"
    }
  ]
}








